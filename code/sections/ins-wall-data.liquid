              "min": 10,
              "max": 90,
              "step": 1,
              "unit": "%"        
          },
          {
            "type": "text",
            "id": "height",
            "label": "height"
          },
          {
            "type": "text",
            "id": "size",
            "label": "size"
          },
          {
            "type": "text",
